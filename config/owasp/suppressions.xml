<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">

  <!-- Suppress Jackson databind false positive -->
  <suppress>
    <notes><![CDATA[
      False Positive
      file name: jackson-databind-2.15.2.jar
    ]]></notes>
    <packageUrl regex="true">^pkg:maven/com\.fasterxml\.jackson\.core/jackson\-databind@.*$</packageUrl>
    <cve>CVE-2023-35116</cve>
  </suppress>

  <!-- Suppress SQLite JDBC false positive -->
  <suppress>
    <notes><![CDATA[
      SQLite JDBC is required for in-memory DB testing.
      HMCTS Java plugin blocks it by default.
    ]]></notes>
    <packageUrl regex="true">^pkg:maven/org\.xerial/sqlite-jdbc@.*$</packageUrl>
  </suppress>

</suppressions>
